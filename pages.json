{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		
		{
			"path": "pages/dynamic/dynamic",
			"style": {
				"navigationStyle":"custom"
			}
		},
		{
			"path": "pages/static/static",
			"style": {
				"navigationBarTitleText": "static"
			}
		},
		{
			"path": "pages/subject/subject",
			"style": {
				"navigationBarTitleText": "subject"
			}
		},
		{
			"path": "pages/widget/widget",
			"style": {
				"navigationBarTitleText": "widget"
			}
		},
		{
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "user"
			}
		}
	],
	
	"tabBar":{
		"list":[
			{
				"pagePath":"pages/dynamic/dynamic"
				
			},
			{
				"pagePath":"pages/static/static"
			
			},
			{
				"pagePath":"pages/subject/subject"
				
			},
			{
				"pagePath":"pages/widget/widget"
			
			},
			{
				"pagePath":"pages/user/user"
			
			}
		]
		
	}
	
	
}
